ducts(data) {\n      this.products = data;\n    },\n    searchFood() {\n      axios\n      .get(\"http://localhost:3000/products?q=\"+this.search)\n      .then((response) => this.setProducts(response.data))\n      .catch((error) => console.log(error));\n    }\n  },\n  mounted() {\n    axios\n      .get(\"http://localhost:3000/best-products\")\n      .then((response) => this.setProducts(response.data))\n      .catch((error) => console.log(error));\n  },\n};\n</script>"]}]}